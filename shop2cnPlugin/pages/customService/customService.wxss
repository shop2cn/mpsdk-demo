/* pages/shop2cnCustomerService/shop2cnCustomerService.wxss */
page {
  width: 100%;
  height: 100%;
  background: white;
}

.container {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
  color: #090A0E;
  font-size: 32rpx;
  padding: 0;
  margin: 0;
}

.img {
  width: 240rpx;
  height: 240rpx;
  margin-bottom: 40rpx;
}

.btn {
  margin-top: 72rpx;
  width: 356rpx;
  height: 80rpx;
  border-radius: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  color: white;
  font-family: 'PingFangSC-Medium';
  background: #F23340;
}

button::after {
  border: none;
}